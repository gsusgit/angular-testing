  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "lint": "eslint . --ext ts --quiet --fix",
    "prerelease": "standard-version ",
    "release": "git push --follow-tags origin main",
    "predeploy": "npm run release",
    "deploy": "ng deploy --base-href=/angular-budget/",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e:prod": "cypress run --config-file=cypress-prod.json",
    "api": "json-server-auth ./database/db.json -r ./database/routes.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/common": "~12.2.8",
