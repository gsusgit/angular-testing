    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "~3.8.0",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "karma": "~6.3.0",
